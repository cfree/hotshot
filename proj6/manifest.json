{
	"name": "A jQuery Chrome Extension",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Scrape web pages for Schema.org micro-data",
	"browser_action": {
		"default_popup": "index.html"
	},
	"sandbox": {
		"pages": ["template.html"]
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"css": ["content.css"],
		"js": ["jquery-1.8.2.min.js", "content.js"]
	}],
	"background": {
		"scripts": ["jquery-1.8.2.min.js", "background.js"]
	}
}